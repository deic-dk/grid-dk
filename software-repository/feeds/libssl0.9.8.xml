    <implementation arch="Linux-x86_64" id="sha1new=5879a85bf82fae45faf1e4cd4b886f5867eb30f6" released="2009-05-27" version="0.9.8">
      <archive href="http://ftp.debian.org/debian/pool/main/o/openssl/libssl0.9.8_0.9.8g-15_amd64.deb" size="975060"/>
    </implementation>
  </group>

</interface>
